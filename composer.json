{
    "name": "dnadesign/silverstripe-elemental-media",
    "description": "A media object element to position an image or video alongside content",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "media",
        "media object",
        "image",
        "video",
        "youtube"
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^6",
        "dnadesign/silverstripe-lazyloaded-image": "^1",
        "dnadesign/silverstripe-youtube-embed": "^1",
        "silverstripe/linkfield": "^5.1",
        "silvershop/silverstripe-hasonefield": "^5"
    },
    "extra": {
        "installer-name": "silverstripe-elemental-media",
        "expose": [
            "client"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dnadesign/silverstripe-youtube-embed"
        }
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev"
}
